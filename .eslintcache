nconst Wrapper = styled.div`\n    height: 88.6%;\n    width: 100%;\n    text-align: center;\n    margin: auto;\n    position: center;\n    display: flex;\n`\n\nconst SignupForm = styled.form`\n    position: relative;\n    width: 60rem;\n    height: 90%;\n    top: 5%;\n    margin: auto;\n    font-weight: 300;\n    text-align: center;\n    padding: 20px;\n    background-color: transparent;\n    border-radius: 5%;\n    \n`\n\nconst Title = styled.h1`\nfont-weight: 700;\nfont-size: 70px;\ncolor: #25ced1;\ntext-shadow: 0px 0px 6px #25ced1;\n\n`\nconst Input = styled.input`\n-webkit-appearance: none;\n     -moz-appearance: none;\n          appearance: none;\n  outline: 0;\n  color: #ffc2e0;\n  border: 1px solid rgba(255, 255, 255, 0.4);\n  background-color: rgba(255, 255, 255, 0.2);\n  width: 250px;\n  border-radius: 3px;\n  padding: 10px 15px;\n  margin: 0 auto 10px auto;\n  display: block;\n  text-align: center;\n  font-size: 18px;\n  transition-duration: 0.25s;\n  font-weight: 300;\n\n\n  ::placeholder{\n      color: #ffc2e0;\n  }\n:hover{\n    background-color: rgba(255, 255, 255, 0.4);\n}\n\n:focus{\n    background-color: white;\n  width: 260px;\n  color: black;\n}\n`\n\nconst InputBio = styled.textarea`\n-webkit-appearance: none;\n     -moz-appearance: none;\n          appearance: none;\n  outline: 0;\n  border: 1px solid rgba(255, 255, 255, 0.4);\n  background-color: rgba(255, 255, 255, 0.2);\n  width: 600px;\n  height: 309px;\n  border-radius: 3px;\n  padding: 0;\n  margin: 0 auto 10px auto;\n  display: block;\n  text-align: left;\n  font-size: 18px;\n  transition-duration: 0.25s;\n  font-weight: 300;\n\n  color: #ffc2e0;\n  ::placeholder{\n      color: #ffc2e0;\n  }\n:hover{\n    background-color: rgba(255, 255, 255, 0.4);\n}\n\n:focus{\n    background-color: white;\n  color: black;\n}\n`\n\nconst Button = styled.button`\n\nbackground-color: #ffcb77;\nborder: 0;\npadding: 10px 15px;\ncolor: black;\nborder-radius: 3px;\nwidth: 250px;\ntext-align: center;\ncursor: pointer;\nfont-size: 18px;\nfont-weight: bold;\ntransition-duration: 0.25s;\nmargin-right: 170px;\nmargin-top: 20px;\n\n}\n\ncolor: var(--color);\n    transition: 0.25s;\n    border-color: var(--hover);\n    color: black;\n    \n    --color: #ffcb77;\n    --hover: #ffcb77;\n    :hover,:focus {\n        border-color: #ffcb77;\n        -webkit-animation: pulse 1s;\n          animation: pulse 1s;\n        box-shadow: 0 0 0 2em rgba(255, 255, 255, 0);\n    }\n\n    @-webkit-keyframes pulse {\n        0% {\n            box-shadow: 0 0 0 0 var(--hover);\n        }\n    }\n\n    @keyframes pulse {\n        0% {\n            box-shadow: 0 0 0 0 var(--hover);\n        }\n    }\n`\n\n\nfunction mdp(dispatch) {\n    return {\n        currentUser: (userObj, history) => dispatch(setUser(userObj, history))\n    }\n}\n\nexport default withRouter(connect(null, mdp)(Signup))","/Users/kguerra/Flatiron/code/Mod5/OurThoughts/ourthoughts-frontend/src/Component/Login.js",["96"],"import React from 'react'\nimport { connect } from 'react-redux'\nimport { setUser } from '../Redux/actions'\nimport { InputText } from 'primereact/inputtext';\nimport { withRouter } from 'react-router'\nimport '../Styles/Login.scss'\nimport styled from \"styled-components\";\n\n\nclass Login extends React.Component {\n\n    state = {\n        username: \"\",\n        password: \"\"\n    }\n\n    changeHandler = (e) => {\n        this.setState({ [e.target.name]: e.target.value })\n    }\n\n    loginHandler = (e) => {\n        e.preventDefault()\n        this.props.currentUser(this.state, this.props.history)\n    }\n\n    render() {\n        return (\n            <Wrapper>\n                <LoginForm className=\"form\" onSubmit={this.loginHandler}>\n                    <Title> Welcome Back </Title>\n                    <br />\n                    <br /><br />\n                    {/* <label htmlFor=\"username\">USERNAME</label> */}\n                    <Input className=\"input\" placeholder=\"Username\" type=\"text\" name=\"username\" value={this.state.username} onChange={this.changeHandler} required />\n                    {/* <label htmlFor=\"password\">PASSWORD</label> */}\n                    <Input className=\"input\" placeholder=\"Password\" type=\"password\" name=\"password\" value={this.state.password} onChange={this.changeHandler} required />\n                    <br /> <br />\n                    <Button className=\"button\"> LOG IN </Button>\n                </LoginForm>\n            </Wrapper>\n        )\n    }\n}\n\nconst Wrapper = styled.div`\n    height: 88.6%;\n    width: 100%;\n    text-align: center;\n    margin: auto;\n    position: center;\n`\n\nconst LoginForm = styled.form`\n    position: relative;\n    width: 60rem;\n    top: 20%;\n    margin: auto;\n    font-weight: 300;\n    text-align: center;\n    padding: 20px;\n    background-color: transparent;\n    border-radius: 5%;\n    \n`\n\nconst Title = styled.h1`\nfont-weight: 700;\nfont-size: 70px;\ncolor: #25ced1;\ntext-align: center;\ntext-shadow: 0px 0px 6px #25ced1;\n\n`\nconst Input = styled.input`\n-webkit-appearance: none;\n     -moz-appearance: none;\n          appearance: none;\n  outline: 0;\n  border: 1px solid rgba(255, 255, 255, 0.4);\n  background-color: rgba(255, 255, 255, 0.2);\n  width: 250px;\n  border-radius: 3px;\n  padding: 10px 15px;\n  margin: 0 auto 10px auto;\n  display: block;\n  color: #ffc2e0;\n  text-align: center;\n  font-size: 18px;\n  transition-duration: 0.25s;\n  font-weight: 300;\n  ::placeholder{\n      color: #ffc2e0;\n  }\n:hover{\n    background-color: rgba(255, 255, 255, 0.4);\n}\n\n:focus{\n    background-color: white;\n  width: 300px;\n  color: black;\n}\n`\n\nconst Button = styled.button`\n\nbackground-color: #ffcb77;\nborder: 0;\npadding: 10px 15px;\ncolor: black;\nborder-radius: 3px;\nwidth: 250px;\ncursor: pointer;\nfont-size: 18px;\ntransition-duration: 0.25s;\nfont-weight: bold;\n\n}\n\ncolor: var(--color);\n    transition: 0.25s;\n    border-color: var(--hover);\n    color: black;\n    --color: #ffcb77;\n    --hover: #ffcb77;\n    :hover,:focus {\n        border-color: #ffcb77;\n        -webkit-animation: pulse 1s;\n          animation: pulse 1s;\n        box-shadow: 0 0 0 2em rgba(255, 255, 255, 0);\n    }\n\n    @-webkit-keyframes pulse {\n        0% {\n            box-shadow: 0 0 0 0 var(--hover);\n        }\n    }\n\n    @keyframes pulse {\n        0% {\n            box-shadow: 0 0 0 0 var(--hover);\n        }\n    }\n`\n\nfunction mdp(dispatch) {\n    return {\n        currentUser: (userObj, history) => dispatch(setUser(userObj, history))\n    }\n}\n\nexport default withRouter(connect(null, mdp)(Login))\n","/Users/kguerra/Flatiron/code/Mod5/OurThoughts/ourthoughts-frontend/src/Redux/actions.js",[],"/Users/kguerra/Flatiron/code/Mod5/OurThoughts/ourthoughts-frontend/src/Redux/actionTypes.js",[],"/Users/kguerra/Flatiron/code/Mod5/OurThoughts/ourthoughts-frontend/src/Containers/ExploreThoughts.js",[],"/Users/kguerra/Flatiron/code/Mod5/OurThoughts/ourthoughts-frontend/src/Component/ThoughtCards.js",[],"/Users/kguerra/Flatiron/code/Mod5/OurThoughts/ourthoughts-frontend/src/Component/CreateThought.js",["97"],"/Users/kguerra/Flatiron/code/Mod5/OurThoughts/ourthoughts-frontend/src/Component/CreateLetter.js",[],"/Users/kguerra/Flatiron/code/Mod5/OurThoughts/ourthoughts-frontend/src/Component/ProfilePage.js",["98"],"import React from 'react'\nimport { connect } from 'react-redux'\nimport { NavLink } from 'react-router-dom'\nimport { deleteUser } from '../Redux/actions'\nimport { withRouter } from 'react-router'\nimport Talk from \"talkjs\";\n\n\nclass ProfilePage extends React.Component {\n\n    deleteHandler = () => {\n        this.props.deleteUser(this.props.currentUser.id, this.props.history)\n    }\n\n    userFinder = () => {\n        if (this.props.location.aboutProps) {\n            return this.props.location.aboutProps.user\n        }\n        else {\n            return this.props.currentUser\n\n        }\n    }\n\n    messageButtonHandler = () => {\n        // * Retrieve the two users that will participate in the conversation * /\n        const currentUser = this.props.currentUser\n        const user = this.userFinder()\n        var other = new Talk.User({\n                id: user.id,\n                name: user.name,\n                email: user.email,\n                photoUrl: user.avatar,\n                welcomeMessage: \"Hey!\"\n            });\n\n        /* Session initialization code */\n        Talk.ready\n            .then(() => {\n                /* Create the two users that will participate in the conversation */\n                const me = new Talk.User(currentUser);\n                // const other = new Talk.User(user)\n\n                /* Create a talk session if this does not exist. Remember to replace tthe APP ID with the one on your dashboard */\n                if (!window.talkSession) {\n                    window.talkSession = new Talk.Session({\n                        appId: process.env.REACT_APP_API_KEY,\n                        me: me\n                    });\n                }\n\n                /* Get a conversation ID or create one */\n                const conversationId = Talk.oneOnOneId(me, other);\n                const conversation = window.talkSession.getOrCreateConversation(conversationId);\n\n                /* Set participants of the conversations */\n                conversation.setParticipant(me);\n                conversation.setParticipant(other);\n\n                /* Create and mount chatbox in container */\n                this.chatbox = window.talkSession.createChatbox(conversation);\n                this.chatbox.mount(this.container);\n            })\n            .catch(e => console.error(e));\n\n    }\n\n    render() {\n        let userObj = this.userFinder()\n        return (\n            <div>\n                <div>\n                    <img src={userObj.avatar} />\n                    <h2>{userObj.username}</h2>\n\n                    {userObj.id === this.props.currentUser.id ?\n                        <>\n                            <br /><br />\n                            <button> INBOX </button>\n                            <NavLink to={{ pathname: \"/edit\", aboutProps: { user: userObj } }}>\n                                <button> EDIT MY ACCOUNT </button>\n                            </NavLink>\n                            <button onClick={this.deleteHandler}> DELETE MY ACCOUNT </button>\n\n                            <NavLink to=\"/past-thoughts\">\n                                <button> MY THOUGHTS </button>\n                            </NavLink>\n                        </>\n                        :\n                        <>\n                            <br /><br />\n                            <div className=\"user-action\">\n                                <button onClick={(userObj) => this.messageButtonHandler(userObj.id)}>Message</button>\n                            </div>\n\n                        </>\n                    }\n                </div>\n\n                <div>\n                    <br /><br />\n                    <h2>NAME: {userObj.name}</h2>\n                    <br />\n                    <h2>PRONOUNS: {userObj.pronouns}</h2>\n                    <br /><br />\n                    <h2>BIO</h2>\n                    <p>{userObj.bio}</p>\n                </div>\n\n\n                <div className=\"chatbox-container\" ref={c => this.container = c}>\n                    <div id=\"talkjs-container\" style={{ height: \"300px\" }}><i></i></div>\n                </div>\n            </div>\n\n\n        )\n    }\n}\n\nfunction msp(state) {\n    return ({\n        currentUser: state.currentUser\n    })\n}\n\nfunction mdp(dispatch) {\n    return ({\n        deleteUser: (userId, history) => dispatch(deleteUser(userId, history))\n    })\n}\nexport default withRouter(connect(msp, mdp)(ProfilePage))","/Users/kguerra/Flatiron/code/Mod5/OurThoughts/ourthoughts-fron