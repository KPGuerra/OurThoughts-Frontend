ar()\n        window.location.href = '/home'\n    }\n\n    render() {\n        return (\n            <div>\n                {/* <NavLink to=\"/home\">\n                    <h1>OUR THOUGHTS</h1>\n        </NavLink> */}\n\n                {this.props.currentUser ?\n                    <>\n                        {/* <NavLink to={{ pathname: \"/profile\", aboutProps: { user: this.props.currentUser } }}>\n                            <h4> {this.props.currentUser.username} </h4>\n                        </NavLink>\n                        <NavLink to=\"/inbox\">\n                            <h4> Inbox </h4>\n                        </NavLink>\n                        <NavLink to=\"/my-thought\">\n                            <h4> Post A Thought </h4>\n                        </NavLink>\n                        <NavLink to=\"/explore-thoughts\">\n                            <h4> Explore Thoughts </h4>\n                        </NavLink>\n                        <NavLink to=\"/resources\">\n                            <h4> Mental Resources </h4>\n                        </NavLink> */}\n\n                        <Menubar model={this.loggedInMenu} end={<Button label=\"Log Out\" onClick={this.logoutHandler} />} />\n                    </>\n                    : <Menubar model={this.loggedOutMenu} /> }\n\n            </div>\n        )\n    }\n}\n\nfunction msp(state) {\n    return ({\n        currentUser: state.currentUser\n    })\n}\nexport default connect(msp)(Header)","/Users/kguerra/Flatiron/code/Mod5/OurThoughts/ourthoughts-frontend/src/Component/HomePage.js",[],"/Users/kguerra/Flatiron/code/Mod5/OurThoughts/ourthoughts-frontend/src/Component/Signup.js",["100"],"import React from 'react'\nimport { connect } from 'react-redux'\nimport { setUser } from '../Redux/actions'\nimport { InputText } from 'primereact/inputtext';\nimport { withRouter } from 'react-router'\nimport '../Styles/Login.scss'\nimport styled from \"styled-components\";\n\nclass Signup extends React.Component 