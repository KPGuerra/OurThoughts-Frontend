n            label: 'My Inbox',\n            icon: 'pi pi-fw pi-inbox',\n            command: () => { window.location.href = \"/inbox\" }\n        },\n        {\n            label: 'Post A Thought',\n            icon: 'pi pi-fw pi-send',\n            command: () => { window.location.href = \"/my-thought\" }\n        },\n        {\n            label: 'Explore Thoughts',\n            icon: 'pi pi-fw pi-cloud',\n            command: () => { window.location.href = \"/explore-thoughts\" }\n        },\n        {\n            label: 'Resources',\n            icon: 'pi pi-fw pi-heart',\n            command: () => { window.location.href = \"/resources\" }\n        },\n\n    ]\n\n    logoutHandler = () => {\n        localStorage.clear()\n        window.location.href = '/home'\n    }\n\n    render() {\n        return (\n            <div>\n                {/* <NavLink to=\"/home\">\n                    <h1>OUR THOUGHTS</h1>\n        </NavLink> */}\n\n                {this.props.currentUser ?\n                    <>\n                        {/* <NavLink to={{ pathname: \"/profile\", aboutProps: { user: this.props.currentUser } }}>\n                            <h4> {this.props.currentUser.username} </h4>\n                        </NavLink>\n                        <NavLink to=\"/inbox\">\n                            <h4> Inbox </h4>\n                        </NavLink>\n                        <NavLink to=\"/my-thought\">\n                            <h4> Post A Thought </h4>\n                        </NavLink>\n                        <NavLink to=\"/explore-thoughts\">\n                            <h4> Explore Thoughts </h4>\n                        </NavLink>\n                        <NavLink to=\"/resources\">\n                            <h4> Mental Resources </h4>\n                        </NavLink> */}\n\n                        <Menubar model={this.loggedInMenu} end={<Button label=\"Log Out\" onClick={this.logoutHandler} />} />\n                    </>\n                    : <Menubar model={this.loggedOutMenu} /> }\n\n            </div>\n        )\n    }\n}\n\nfunction msp(state) {\n    return ({\n        currentUser: state.currentUser\n    })\n}\nexport default connect(msp)(Header)","/Users/kguerra/Flatiron/code/Mod5/OurThoughts/ourthoughts-frontend/src/Component/HomePage.js",[],"/Users/kguerra/Flatiron/code/Mod5/OurThoughts/ourthoughts-frontend/src/Component/Signup.js",[],"/Users/kguerra/Flatiron/code/Mod5/OurThoughts/ourthoughts-frontend/src/Component/Login.js",[],"/Users/kguerra/Flatiron/code/Mod5/OurThoughts/ourthoughts-frontend/src/Redux/actions.js",[],"/Users/kguerra/Flatiron/code/Mod5/OurThoughts/ourthoughts-frontend/src/Redux/actionTypes.js",[],"/Users/kguerra/Flatiron/code/Mod5/OurThoughts/ourthoughts-frontend/src/Containers/ExploreThoughts.js",[],"/Users/kguerra/Flatiron/code/Mod5/OurThoughts/ourthoughts-frontend/src/Component/ThoughtCards.js",[],"/Users/kguerra/Flatiron/code/Mod5/OurThoughts/ourthoughts-frontend/src/Component/CreateThought.js",[],"/Users/kguerra/Flatiron/code/Mod5/OurThoughts/ourthoughts-frontend/src/Component/CreateLetter.js",[],"/Users/kguerra/Flatiron/code/Mod5/OurThoughts/ourthoughts-frontend/src/Component/ProfilePage.js",["93"],"import React from 'react'\nimport { connect } from 'react-redux'\nimport { NavLink } from 'react-router-dom'\nimport { deleteUser } from '../Redux/actions'\nimport { withRouter } from 'react-router'\nimport Talk from \"talkjs\";\n\n\nclass ProfilePage extends React.Component {\n\n    deleteHandler = () => {\n        this.props.deleteUser(this.props.currentUser.id, this.props.history)\n    }\n\n    userFinder = () => {\n        if (this.props.location.aboutProps) {\n            return this.props.location.aboutProps.user\n        }\n        else {\n            return this.props.currentUser\n\n        }\n    }\n\n    messageButtonHandler = () => {\n        // * Retrieve the two users that will participate in the conversation * /\n        const currentUser = this.props.currentUser\n        const user = this.userFinder()\n        var other = new Talk.User({\n                id: user.id,\n                name: user.name,\n                email: user.email,\n                photoUrl: user.avatar,\n                welcomeMessage: \"Hey!\"\n            });\n\n        /* Session initialization code */\n        Talk.ready\n            .then(() => {\n                /* Create the two users that will participate in the conversation */\n                const me = new Talk.User(currentUser);\n                // const other = new Talk.User(user)\n\n                /* Create a talk session if this does not exist. Remember to replace tthe APP ID with the one on your dashboard */\n                if (!window.talkSession) {\n                    window.talkSession = new Talk.Session({\n                        appId: process.env.REACT_APP_API_KEY,\n                        me: me\n                    });\n                }\n\n                /* Get a conversation ID or create one */\n                const conversationId = Talk.oneOnOneId(me, other);\n                const conversation = window.talkSession.getOrCreateConversation(conversationId);\n\n                /* Set participants of the conversations */\n                conversation.setParticipant(me);\n                conversation.setParticipant(other);\n\n                /* Create and mount chatbox in container */\n                this.chatbox = window.talkSession.createChatbox(conversation);\n                this.chatbox.mount(this.container);\n            })\n            .catch(e => console.error(e));\n\n    }\n\n    render() {\n        let userObj = this.userFinder()\n        return (\n            <div>\n                <div>\n                    <img src={userObj.avatar} />\n                    <h2>{userObj.username}</h2>\n\n                    {userObj.id === this.props.currentUser.id ?\n                        <>\n                            <br /><br />\n                            <button> INBOX </button>\n                            <NavLink to={{ pathname: \"/edit\", aboutProps: { user: userObj } }}>\n                                <button> EDIT MY ACCOUNT </button>\n                            </NavLink>\n                            <button onClick={this.deleteHandler}> DELETE MY ACCOUNT </button>\n\n                            <NavLink to=\"/past-thoughts\">\n                                <button> MY THOUGHTS </button>\n                            </NavLink>\n                        </>\n                        :\n                        <>\n                            <br /><br />\n                            <div className=\"user-action\">\n                                <button onClick={(userObj) => this.messageButtonHandler(userObj.id)}>Message</button>\n                            </div>\n\n                        </>\n                    }\n                </div>\n\n                <div>\n                    <br /><br />\n                    <h2>NAME: {userObj.name}</h2>\n                    <br />\n                    <h2>PRONOUNS: {userObj.pronouns}</h2>\n                    <br /><br />\n                    <h2>BIO</h2>\n                    <p>{userObj.bio}</p>\n                </div>\n\n\n                <div className=\"chatbox-container\" ref={c => this.container = c}>\n                    <div id=\"talkjs-container\" style={{ height: \"300px\" }}><i></i></div>\n                </div>\n            </div>\n\n\n        )\n    }\n}\n\nfunction msp(state) {\n    return ({\n        currentUser: state.currentUser\n    })\n}\n\nfunction mdp(dispatch) {\n    return ({\n        deleteUser: (userId, history) => dispatch(deleteUser(userId, history))\n    })\n}\nexport default withRouter(connect(msp, mdp)(ProfilePage))","/Users/kguerra/Flatiron/code/Mod5/OurThoughts/ourthoughts-frontend/src/Component/PastThoughts.js",[],"/Users/kguerra/Flatiron/code/Mod5/OurThoughts/ourthoughts-frontend/src/Containers/ThoughtPage.js",[],"/Users/kguerra/Flatiron/code